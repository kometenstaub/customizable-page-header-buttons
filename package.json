{
	"name": "customizable-page-header-buttons",
	"version": "4.0.0",
	"description": "This plugin lets you add buttons for executing commands to the page header.",
	"main": "src/main.ts",
	"scripts": {
		"dev": "cross-env BUILD=dev node esbuild.mjs",
		"build": "cross-env BUILD=production node esbuild.mjs",
		"release": "standard-version",
		"format": "npx prettier --write src/"
	},
	"standard-version": {
		"t": "",
		"types": [
			{
				"type": "perf",
				"hidden": false,
				"section": "Performance"
			},
			{
				"type": "feat",
				"hidden": false,
				"section": "Features"
			},
			{
				"type": "fix",
				"hidden": false,
				"section": "Bug Fixes"
			}
		],
		"releaseCommitMessageFormat": "Customizable Page Header: {{currentTag}}"
	},
	"keywords": [],
	"author": "kometenstaub",
	"license": "MIT",
	"devDependencies": {
		"@types/feather-icons": "^4.7.0",
		"@types/node": "^14.14.37",
		"cross-env": "^7.0.3",
		"esbuild": "0.14.3",
		"obsidian": "^0.12.15",
		"prettier": "2.5.1",
		"standard-version": "^9.3.1",
		"tslib": "^2.2.0",
		"typescript": "^4.2.4"
	},
	"dependencies": {
		"feather-icons": "^4.28.0"
	}
}
